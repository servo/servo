        <link rel="match" href="../reference/ref-filled-black-96px-square.xht" />

        <meta name="assert" content="The 'max-width' property applies a value of 'none'." />
        <style type="text/css">
            div
            {
                background: black;
                height: 1in;
                max-width: 0px;
                max-width: none;
                width: 1in;
            }
        </style>
