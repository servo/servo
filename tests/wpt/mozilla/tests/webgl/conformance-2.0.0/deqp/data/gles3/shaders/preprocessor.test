			}
		""
	end

	case undefine_function
		version 300 es
		values { output float out0 = 1.0; }
		both ""
			#version 300 es
			precision mediump float;
			#define		FUNCTION(a,b) a+b
			#undef		FUNCTION
			#define 	FUNCTION(a,b) a-b
