   The cascade is as follows:
      HTTP Link: header
       @import
   a    Rule
       @import
        @import
   b     Rule
   c    Rule
   d   Rule
      HTTP Link: header
       @import
   e    Rule
   f   Rule
      <meta http-equiv="Link">
       @import
   g    Rule
   h   Rule
      <link rel="stylesheet">
       @import
   i    Rule
   j   Rule
      <style type="text/css">
       @import
   k    Rule
   l   Rule
      <link rel="stylesheet">
       @import
        @import
