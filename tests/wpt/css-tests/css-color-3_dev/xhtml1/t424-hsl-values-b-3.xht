      <td style="background: rgb(255, 0, 153)">&#xA0;</td>
      <td style="background: rgb(255, 0, 102)">&#xA0;</td>
      <td style="background: rgb(255, 0, 51)">&#xA0;</td>
      <td style="background: rgb(255, 0, 0)">&#xA0;</td>
      <td style="background: black">&#xA0;</td>
    </tr>
