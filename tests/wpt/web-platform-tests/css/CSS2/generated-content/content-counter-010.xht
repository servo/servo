  <meta http-equiv="Content-Style-Type" content="text/css"/>
  <style type="text/css">

  body { white-space: nowrap; }


  #test { counter-reset: c; }
  #test span { counter-increment: c; }
  #test span:before { content: counter(c, armenian); }

  </style>
