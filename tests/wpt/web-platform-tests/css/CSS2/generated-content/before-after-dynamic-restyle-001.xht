<style type="text/css">
body:before {
  content:"Before";
  border:inherit;
}
.cl:after {
  display:block;
  content:"After";
}
</style>
<script type="text/javascript">
function fixupDOM() {
  document.body.setAttribute("style", "border:2px solid green;");
  document.body.className = "cl";
  document.documentElement.className = "";
}
</script>
</head>
<body onload="fixupDOM()">


