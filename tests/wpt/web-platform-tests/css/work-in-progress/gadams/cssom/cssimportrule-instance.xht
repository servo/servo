<!DOCTYPE html PUBLIC "-//W3C//DTD//XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <title>CSSOM - CSSImportRule - Instance</title>
    <link rel="author" title="Glenn Adams" href="mailto:glenn@skynav.com"/>
    <link rel="help" href="http://www.w3.org/TR/cssom-1/#the-cssimportrule-interface"/>
    <meta name="flags" content="dom"/>
    <script src="/resources/testharness.js"/>
    <script src="/resources/testharnessreport.js"/>
    <style id="sheet">
      @import "resources/import.css";
    </style>
  </head>
  <body>
    <div id="log"></div>
    <div id="box"></div>
    <script><![CDATA[

      var doc = window.document;

      test(function(){

        assert_true('styleSheets' in doc);

      }, '\'styleSheets\' in doc');

      var sheets = doc.styleSheets;

      test(function(){

        assert_true(sheets.length > 0);

      }, 'doc.styleSheets.length > 0');

      var sheet = doc.styleSheets[0];

      test(function(){

        assert_true(sheet instanceof CSSStyleSheet);

      }, 'doc.styleSheets[0] instanceof CSSStyleSheet');

      test(function(){

        assert_true('cssRules' in sheet);

      }, '\'cssRules\' in doc.styleSheets[0]');

      var rules = sheet.cssRules;

      test(function(){

        assert_true(rules instanceof CSSRuleList);

      }, 'doc.styleSheets[0].cssRules instanceof CSSRuleList');

      test(function(){

        assert_true(rules.length > 0);

      }, 'doc.styleSheets[0].cssRules.length > 0');

      var rule = rules[0];

      test(function(){

        assert_true(rule instanceof CSSRule);

      }, 'doc.styleSheets[0].cssRules[0] instanceof CSSRule');

      test(function(){

        assert_true(rule instanceof CSSImportRule);

      }, 'doc.styleSheets[0].cssRules[0] instanceof CSSImportRule');

      test(function(){

        assert_equals(rule.toString(), '[object CSSImportRule]');

      }, 'doc.styleSheets[0].cssRules[0].toString() == \'[object CSSImportRule]\'');

      test(function(){

        assert_true('type' in rule);

      }, '\'type\' in doc.styleSheets[0].cssRules[0]');

      test(function(){

        assert_equals(rule.type, 3);

      }, 'doc.styleSheets[0].cssRules[0].type == 3');

      test(function(){

        assert_true('cssText' in rule);

      }, '\'cssText\' in doc.styleSheets[0].cssRules[0]');

      test(function(){

        assert_equals(rule.cssText, '@import url(\"resources/import.css\");');

      }, 'doc.styleSheets[0].cssRules[0].cssText == \'@import url(\"resources/import.css\");\'');

      test(function(){

        assert_true('parentRule' in rule);

      }, '\'parentRule\' in doc.styleSheets[0].cssRules[0]');

      test(function(){

        assert_true(rule.parentRule === null);

      }, 'doc.styleSheets[0].cssRules[0].parentRule === null');

      test(function(){

        assert_true('parentStyleSheet' in rule);

      }, '\'parentStyleSheet\' in doc.styleSheets[0].cssRules[0]');

      test(function(){

        assert_true(rule.parentStyleSheet === sheet);

      }, 'doc.styleSheets[0].cssRules[0].parentStyleSheet === doc.styleSheets[0]');

      test(function(){

        assert_true('href' in rule);

      }, '\'href\' in doc.styleSheets[0].cssRules[0]');

      test(function(){

        assert_true(typeof(rule.href) == 'string');

      }, 'typeof(doc.styleSheets[0].cssRules[0].href) == \'string\'');

      test(function(){

        assert_true('media' in rule);

      }, '\'media\' in doc.styleSheets[0].cssRules[0]');

      var media = rule.media;

      test(function(){

        assert_true(media instanceof MediaList);

      }, 'doc.styleSheets[0].cssRules[0].media instanceof MediaList');

      test(function(){

        assert_true('mediaText' in media);

      }, '\'mediaText\' in doc.styleSheets[0].cssRules[0].media');

      test(function(){

        assert_equals(typeof(media.mediaText), 'string');

      }, 'typeof(doc.styleSheets[0].cssRules[0].media.mediaText) == \'string\'');

      test(function(){

        assert_equals(media.mediaText, '');

      }, 'doc.styleSheets[0].cssRules[0].media.mediaText == \'\'');

      test(function(){

        assert_true('styleSheet' in rule);

      }, '\'styleSheet\' in doc.styleSheets[0].cssRules[0]');

      var sheetImported = rule.styleSheet;

      test(function(){

        assert_true(sheetImported instanceof CSSStyleSheet);

      }, 'doc.styleSheets[0].cssRules[0].styleSheet instanceof CSSStyleSheet');

      test(function(){

        assert_true('cssRules' in sheetImported);

      }, '\'cssRules\' in doc.styleSheets[0].cssRules[0].styleSheet');

      var rulesImported = sheetImported.cssRules;

      test(function(){

        assert_true(rulesImported instanceof CSSRuleList);

      }, 'doc.styleSheets[0].cssRules[0].styleSheet.cssRules instanceof CSSRuleList');

      test(function(){

        assert_true(rulesImported.length > 0);

      }, 'doc.styleSheets[0].cssRules[0].styleSheet.cssRules.length > 0');

      var ruleImported = rulesImported[0];

      test(function(){

        assert_true(ruleImported instanceof CSSStyleRule);

      }, 'doc.styleSheets[0].cssRules[0].styleSheet.cssRules[0] instanceof CSSStyleRule');

      test(function(){

        assert_true(ruleImported.parentRule === null);

      }, 'doc.styleSheets[0].cssRules[0].styleSheet.cssRules[0].parentRule === null');

      test(function(){

        assert_true(ruleImported.parentStyleSheet === sheetImported);

      }, 'doc.styleSheets[0].cssRules[0].styleSheet.cssRules[0].parentStyleSheet === doc.styleSheets[0].cssRules[0].styleSheet');

    ]]>
    </script>
  </body>
</html>
