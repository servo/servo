<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN" "http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<html><head>
  <link rel="match" href="insert-into-split-inline-11-ref.xht"/>
  <style>
   body &gt; span { border: 3px solid blue }
   body &gt; span &gt; span { border: 3px solid cyan }
   .notstart { border-left: none; }
   .notend { border-right: none; }
  </style>
 </head>
 <body>
  <span class="notend">
    <span class="notend">
      <span>One</span><span>Two</span><span>Three</span>
    </span>
  </span>
  <div>Four</div>
  <span class="notstart notend"><span class="notstart notend"></span></span>
  <div>Five</div>
  <span class="notstart notend">
    <span class="notstart notend">
      <span>Six</span>
    </span>
  </span>
  <div>Seven</div>
  <span class="notstart notend"><span class="notstart notend"></span></span>
  <div>Eight</div>
  <span class="notstart notend">
    <span class="notstart">
      <span>Nine</span>
    </span>
  </span>
  <div>Ten</div>
  <span class="notstart"></span>
 

</body></html>
