namespace ServoApp {

  delegate void EventDelegate();
  delegate void HistoryChangedDelegate(Boolean back, Boolean forward);

  runtimeclass ServoControl : Windows.UI.Xaml.Controls.Control {
      ServoControl();
      void GoBack();
      void GoForward();
      void Reload();
      void Stop();
      Windows.Foundation.Uri LoadURIOrSearch(String url);
